---
import config from '../../config';
---
<div id="not-as-usual" class="cursor-default hover:cursor-default mt-24"><span class="text-[#0d0d12]">-</span></div>
<div>
        <div class="items-center mb-2  flex justify-center flex-col">
            <h2 class="text-xl font-black bg-gradient-to-r from-[#8b9ce1] via-[#a88aef] to-[#dc75fc] text-transparent bg-clip-text">{config.NotAsUsual}</h2>
            <h1 class="mt-4 text-5xl font-bold text-white">{config.NotAsUsualHeading}</h1>
            <div class="mt-10 flex flex-wrap">
                <div class="transition-all duration-500 ease-linear text-white bg-[#171720] lg:text-gray-400 lg:hover:text-white lg:hover:border lg:hover:bg-[#171720] lg:bg-[#0d0d12] border px-6 py-3 rounded-full mt-4 lg:border-gray-700 sm:border-[#E86DFC]/40 lg:hover:border-[#E86DFC]/40"><a href={config.schedulemeetingurl} target="_blank">{config.ScheduleMeetingText}</a></div>
            </div>
    </div>
</div>    