---
import config from '../../config';
---   
<div id="our-program" class="cursor-default hover:cursor-default mb-[120px]"><span class="text-[#0d0d12]">-</span></div>
<div class="mt-20 mb-[12rem] w-full h-full flex lg:flex-col flex-col justify-center items-center">
        <h2 class="text-xl font-black bg-gradient-to-r from-[#075E8C] via-[#0CA2F2] to-[#075E8C] text-transparent bg-clip-text">{config.OurProgram}</h2>
        <h1 class="mt-4 text-4xl md:text-5xl font-bold text-white">{config.OurProgramHeading}</h1>
        <div id="items" class="mt-6 ml-2 mb-[360px] md:mb-[160px] flex flex-wrap justify-center items-center relative"> 
            <div class="my-auto flex z-8 justify-center bg-[#0d0d12] items-center">
                <div class="flex flex-col md:justify-center items-center"> 
                    <div class="flex flex-col items-center md:flex-row my-auto absolute top-[60px] mb-24">
                        <div class="flex w-[400px] md:w-[410px] md:mr-6 my-auto border border-gray-700 rounded-lg px-6 md:py-6 py-8">
                            <svg class="w-14" xmlns="http://www.w3.org/2000/svg" width="" height="" viewBox="0 0 24 24" style="fill: rgba(255, 255, 255, 1);transform: ;msFilter:;"><path d="M20 10.999h2C22 5.869 18.127 2 12.99 2v2C17.052 4 20 6.943 20 10.999z"></path><path d="M13 8c2.103 0 3 .897 3 3h2c0-3.225-1.775-5-5-5v2zm3.422 5.443a1.001 1.001 0 0 0-1.391.043l-2.393 2.461c-.576-.11-1.734-.471-2.926-1.66-1.192-1.193-1.553-2.354-1.66-2.926l2.459-2.394a1 1 0 0 0 .043-1.391L6.859 3.513a1 1 0 0 0-1.391-.087l-2.17 1.861a1 1 0 0 0-.29.649c-.015.25-.301 6.172 4.291 10.766C11.305 20.707 16.323 21 17.705 21c.202 0 .326-.006.359-.008a.992.992 0 0 0 .648-.291l1.86-2.171a1 1 0 0 0-.086-1.391l-4.064-3.696z"></path></svg>
                            <div class="ml-4 flex flex-col">
                                <a href="discord.com" target="_blank"><h2 class="text-[13px] hover:text-[#8893f3] transition-colors duration-1000 ease-linear text-[#d3d3d3] font-bold flex">{config.FirstCardTitle}</h2></a>
                                <p class="text-[14px] font-regular text-[#d3d3d3]/90">{config.FirstCardDesc}</p>
                            </div>
                        </div>
                        <div class="mt-8 md:mt-0 flex w-[400px] md:w-[410px] my-auto border border-gray-700 rounded-lg px-6 md:py-6 py-8">
                            <svg class="w-14" xmlns="http://www.w3.org/2000/svg" width="" height="" viewBox="0 0 24 24" style="fill: rgba(255, 255, 255, 1);transform: ;msFilter:;"><path d="M3 5v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2V5a2 2 0 0 0-2-2H5a2 2 0 0 0-2 2zm7 2h8v2h-8V7zm0 4h8v2h-8v-2zm0 4h8v2h-8v-2zM6 7h2v2H6V7zm0 4h2v2H6v-2zm0 4h2v2H6v-2z"></path></svg>
                            <div class="ml-4 flex flex-col">
                                <h2 class="cursor-pointer text-[13px] transition-colors duration-1000 ease-linear text-[#d3d3d3] font-bold flex">{config.SecondCardTitle}</h2>
                                <p class="text-[14px] font-regular text-[#d3d3d3]/90">{config.SecondCardDesc}</p>
                            </div>
                        </div>
                    </div>                       
                    <div class="flex flex-col md:flex-row md:mt-[500px] mt-[375px] my-auto absolute">  
                        <div class="flex w-[400px] md:w-[410px] my-auto md:mr-6 border border-gray-700 rounded-lg px-6 md:py-6 py-8">
                            <svg class="w-14" xmlns="http://www.w3.org/2000/svg" width="" height="" viewBox="0 0 24 24" style="fill: rgba(255, 255, 255, 1);transform: ;msFilter:;"><path d="M12 2C6.486 2 2 6.486 2 12s4.486 10 10 10 10-4.486 10-10S17.514 2 12 2zm1 14.915V18h-2v-1.08c-2.339-.367-3-2.002-3-2.92h2c.011.143.159 1 2 1 1.38 0 2-.585 2-1 0-.324 0-1-2-1-3.48 0-4-1.88-4-3 0-1.288 1.029-2.584 3-2.915V6.012h2v1.109c1.734.41 2.4 1.853 2.4 2.879h-1l-1 .018C13.386 9.638 13.185 9 12 9c-1.299 0-2 .516-2 1 0 .374 0 1 2 1 3.48 0 4 1.88 4 3 0 1.288-1.029 2.584-3 2.915z"></path></svg>
                            <div class="ml-4 flex flex-col">
                                <h2 class="cursor-pointer text-[13px] transition-colors duration-1000 ease-linear text-[#d3d3d3] font-bold flex">{config.ThirdCardTitle}</h2>
                                <p class="text-[14px] font-regular text-[#d3d3d3]/90">{config.ThirdCardDesc}</p>                
                            </div>
                        </div>
                        <div class="mt-8 md:mt-0 flex w-[400px] md:w-[410px] my-auto border border-gray-700 rounded-lg px-6 md:py-6 py-8">
                            <svg class="w-14" xmlns="http://www.w3.org/2000/svg" width="" height="" viewBox="0 0 24 24" style="fill: rgba(255, 255, 255, 1);transform: ;msFilter:;"><path d="M4 21h1V8H4a2 2 0 0 0-2 2v9a2 2 0 0 0 2 2zM20 8h-7l1.122-3.368A2 2 0 0 0 12.225 2H12L7 7.438V21h11l3.912-8.596L22 12v-2a2 2 0 0 0-2-2z"></path></svg>
                            <div class="ml-4 flex flex-col">
                                <h2 class="cursor-pointer text-[13px] transition-colors duration-1000 ease-linear text-[#d3d3d3] font-bold flex">{config.FourthCardTitle}</h2>
                                <p class="text-[14px] font-regular text-[#d3d3d3]/90">{config.FourthCardDesc}</p>                
                            </div>
                        </div>
                    </div>                            
                </div>
         </div>
     </div> 
</div>